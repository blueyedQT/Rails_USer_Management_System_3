<head>
  <title>Two Layouts</title>
  <%= stylesheet_link_tag 'two_column', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'two_column', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<div class="row">
			<div id="left" class="col-md-8">
				<%= yield :left %>
			</div>
			<div id="right" class="col-md-4">
			<%= yield :right %>
			</div>
		</div>
</body>